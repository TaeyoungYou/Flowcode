begin:
    declaration:
        int a, b, c, result
        double x, y, finalValue
        string msg
    end;

    boolean Main():
        a = 10
        b = 5
        c = 3
        x = 2.0
        y = 4.5

        result = CalcA: a + b * 2, c + 1, a - c;
        result = result + 10 * (a - b)

        finalValue = Adjust: x + y, x * y, result;
        finalValue = finalValue * 1.1 + result / 2

        msg = 'COMPUTATION RESULT:'
        Output: msg;
        Output: result;

        msg = 'FINAL VALUE:'
        Output: msg;
        Output: finalValue;

        return 'true'
    end;

    int CalcA(int p, int q, int r):
        return (p + q * r) / (p - r + 1)
    end;

    double Adjust(double m, double n, int ref):
        return ((m * n) / (ref + 1)) + 0.75
    end;
end;
